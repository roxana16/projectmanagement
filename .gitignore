# gitIgnore for java projects, with additional ignores for intellij



# Compiled class file
*.class

# Log file
*.log

# BlueJ files
*.ctxt

# Mobile Tools for Java (J2ME)
.mtj.tmp/

# Package Files #
*.jar
*.war
*.nar
*.ear
*.zip
*.tar.gz
*.rar

# virtual machine crash logs, see http://www.java.com/en/download/help/error_hotspot.xml
hs_err_pid*

# Covers JetBrains IDEs: IntelliJ, RubyMine, PhpStorm, AppCode, PyCharm, CLion, Android Studio and WebStorm
# Reference: https://intellij-support.jetbrains.com/hc/en-us/articles/206544839

# User-specific stuff
projectmanagement/.idea/**/workspace.xml
projectmanagement/.idea/**/tasks.xml
projectmanagement/.idea/**/usage.statistics.xml
projectmanagement/.idea/**/dictionaries
projectmanagement/.idea/**/shelf

# Sensitive or high-churn files
projectmanagement/.idea/**/dataSources/
projectmanagement/.idea/**/dataSources.ids
projectmanagement/.idea/**/dataSources.local.xml
projectmanagement/.idea/**/sqlDataSources.xml
projectmanagement/.idea/**/dynamic.xml
projectmanagement/.idea/**/uiDesigner.xml
projectmanagement/.idea/**/dbnavigator.xml

# Gradle
projectmanagement/.idea/**/gradle.xml
projectmanagement/.idea/**/libraries

# Gradle and Maven with auto-import
# When using Gradle or Maven with auto-import, you should exclude module files,
# since they will be recreated, and may cause churn.  Uncomment if using
# auto-import.
projectmanagement/.idea/modules.xml
projectmanagement/.idea/*.iml
projectmanagement/.idea/modules

# CMake
projectmanagement/cmake-build-*/

# Mongo Explorer plugin
projectmanagement/.idea/**/mongoSettings.xml

# File-based project format
projectmanagement/*.iws

# IntelliJ
projectmanagement/out/

# mpeltonen/sbt-idea plugin
projectmanagement/.idea_modules/

# JIRA plugin
projectmanagement/atlassian-ide-plugin.xml

# Cursive Clojure plugin
projectmanagement/.idea/replstate.xml

# Crashlytics plugin (for Android Studio and IntelliJ)
projectmanagement/com_crashlytics_export_strings.xml
projectmanagement/crashlytics.properties
projectmanagement/crashlytics-build.properties
projectmanagement/fabric.properties

# Editor-based Rest Client
projectmanagement/.idea/httpRequests
